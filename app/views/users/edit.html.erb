<h1>ユーザー情報の編集</h1>
<div>
  <%= form_with(model: @user, local: true) do |f| %>
    <div class="form-group">
      <%= f.label :name, 'Name' %>
      <%= f.text_field :name, class: 'form-control' %>
    </div>
    <div class="form-group">
      <p><%= f.label :image, 'Profile Image' %></p>
      <%= f.file_field :image %>
    </div>
    <%= f.submit 'Update', class: 'btn btn-primary btn-block' %>
  <% end %>
</div>